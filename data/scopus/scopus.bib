@ARTICLE{Etheredge201837,
author={Etheredge, R.I. and Avenas, C. and Armstrong, M.J. and Cummings, M.E.},
title={Sex-specific cognitive–behavioural profiles emerging from individual variation in numerosity discrimination in Gambusia affinis},
