	author = {Rozen-Rechels, David and Dupoue, Andreaz and Meylan, S and , rine and Decenciere, Beatriz and , Guing and , Sophie and Le Galliard, Jean-Francois},
	year = {2018},
	keywords = {Lizards, Anxiety, Dehydration},
	pages = {NA},
	annote = { RAYYAN-INCLUSION: "Martin"={\textgreater}true {\textbar} RAYYAN-LABELS: lizards,long timescale,multiple measurements},
