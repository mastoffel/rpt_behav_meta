@article{coyer_post-ice_nodate,
	title = {Post-ice age recolonization and differentiation of {Fucus} serratus {L}. ({Phaeophyceae}; {Fucaceae}) populations in {Northern} {Europe}},
	volume = {12},
	issn = {1365-294X},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1046/j.1365-294X.2003.01850.x},
	doi = {10.1046/j.1365-294X.2003.01850.x},
	abstract = {The seaweed Fucus serratus is hypothesized to have evolved in the North Atlantic and present populations are thought to reflect recolonization from a southern refugium since the last glacial maximum 18 000–20 000 years bp. We examined genetic structure across several spatial scales by analysing seven microsatellite loci in populations collected from 21 localities throughout the species’ range. Spatial auto-correlation analysis of seven microsatellite loci revealed no evidence for spatial clustering of alleles on a scale of 100 m despite limited gamete dispersal in F. serratus of ≈ 2 m from parental individuals. Pairwise θ analysis suggested that the minimal panmictic unit for F. serratus was between 0.5 and 2 km. Isolation by distance was significant along some contiguous coastlines. Population differentiation was strong within the Skagerrak–Kattegat–Baltic Seas (SKB) (global θ= 0.17) despite a short history of ≈ 7500 years. A neighbour-joining tree based on Reynold's distances computed from the microsatellite data revealed a central assemblage of populations on the Brittany Peninsula surrounded by four well-supported clusters consisting of the SKB, the North Sea (Ireland, Helgoland), and two populations from the northern Spanish coast. Samples from Iceland and Nova Scotia were most closely aligned with northwest Sweden and Brittany, respectively. When sample sizes were standardized (N = 41), allelic diversity was twofold higher for Brittany populations than for populations to the north and threefold higher than southern populations. The Brittany region may be a refugium or a recolonized area, whereas the Spanish populations most likely reflect present-day edge populations that have undergone repeated bottlenecks as a consequence of thermally induced cycles of recolonization and extinction.},
	language = {en},
	number = {7},
	urldate = {2018-06-13},
	journal = {Molecular Ecology},
	author = {Coyer, J. A. and Peters, A. F. and Stam, W. T. and Olsen, J. L.},
	keywords = {Fucus serratus, genetic structure, ice ages, microsatellites, seaweeds},
	pages = {1817--1829},
	file = {Full Text PDF:/Users/martin/Zotero/storage/8E6AI3F4/Coyer et al. - Post-ice age recolonization and differentiation of.pdf:application/pdf;Snapshot:/Users/martin/Zotero/storage/3PWPEY9P/j.1365-294X.2003.01850.html:text/html;Snapshot:/Users/martin/Zotero/storage/LQFS72NX/j.1365-294X.2003.01850.html:text/html}
}

@article{liu_late_2006,
	title = {Late {Pleistocene} divergence and subsequent population expansion of two closely related fish species, {Japanese} anchovy ({Engraulis} japonicus) and {Australian} anchovy ({Engraulis} australis)},
	volume = {40},
	issn = {1055-7903},
	url = {http://www.sciencedirect.com/science/article/pii/S1055790306001527},
	doi = {10.1016/j.ympev.2006.04.019},
	abstract = {Climatic oscillations during the Pleistocene ice ages produced great changes in species’ geographical distribution and abundance, which could be expected to have genetic consequences. Living in the temperate upwelling zones of the northwestern Pacific, Japanese anchovy (Engraulis japonicus) might have been affected by these severe climatic oscillations. To investigate the effects of Pleistocene climatic changes on the evolution in Japanese anchovy, fragments of 522bp at the 5′ end of mitochondrial DNA control region were sequenced for 241 individuals from 13 localities and 37 individuals of Australian anchovy. Japanese anchovy and Australian anchovy are reciprocally monophyletic and a late Pleistocene transequatorial divergence between the two species was indicated. High levels of haplotype diversity ({\textgreater}0.99) were found for all samples, indicating a high level of genetic diversity. Analyses of molecular variance and the conventional population statistic FST revealed no significant genetic structure throughout the range of Japanese anchovy. Both mismatch distribution analyses and neutrality tests suggested a late Pleistocene population expansion for both Japanese anchovy (79,000–317,000 years ago) and Australian anchovy (45,000–178,000 years ago).},
	number = {3},
	urldate = {2018-06-13},
	journal = {Molecular Phylogenetics and Evolution},
	author = {Liu, Jin-Xian and Gao, Tian-Xiang and Zhuang, Zhi-Meng and Jin, Xian-Shi and Yokogawa, Koji and Zhang, Ya-Ping},
	month = sep,
	year = {2006},
	keywords = {Climatic oscillations, Mitochondrial DNA, Pleistocene glaciations, Population expansion},
	pages = {712--723},
	file = {ScienceDirect Full Text PDF:/Users/martin/Zotero/storage/24CVPQQ2/Liu et al. - 2006 - Late Pleistocene divergence and subsequent populat.pdf:application/pdf;ScienceDirect Snapshot:/Users/martin/Zotero/storage/XCN8UC5Q/S1055790306001527.html:text/html}
