@article{keller_immigration_2001,
	title = {Immigration and the ephemerality of a natural population bottleneck: evidence from molecular markers},
	volume = {268},
	url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2001.1607},
	doi = {10.1098/rspb.2001.1607},
	abstract = {{\textless}p{\textgreater}Population bottlenecks are often invoked to explain low levels of genetic variation in natural populations, yet few studies have documented the direct genetic consequences of known bottlenecks in the wild. Empirical studies of natural population bottlenecks are therefore needed, because key assumptions of theoretical and laboratory studies of bottlenecks may not hold in the wild. Here we present microsatellite data from a severe bottleneck (95\% mortality) in an insular population of song sparrows (\textit{Melospiza melodia}). The major findings of our study are as follows: (i) The bottleneck reduced heterozygosity and allelic diversity nearly to neutral expectations, despite non–random survival of birds with respect to inbreeding and wing length. (ii) All measures of genetic diversity regained pre–bottleneck levels within two to three years of the crash. This rapid recovery was due to low levels of immigration. (iii) The rapid recovery occurred despite a coincident, strong increase in average inbreeding. These results show that immigration at levels that are hard to measure in most field studies can lead to qualitatively very different genetic outcomes from those expected from mutations only. We suggest that future theoretical and empirical work on bottlenecks and metapopulations should address the impact of immigration.{\textless}/p{\textgreater}},
	language = {English},
	number = {1474},
	journal = {Proceedings. Biological sciences / The Royal Society},
	author = {Keller, Lukas F and Jeffery, Kathryn J and Arcese, Peter and Beaumont, Mark A and Hochachka, Wesley M and Smith, James N M and Bruford, Michael W},
	month = jul,
	year = {2001},
	pmid = {11429139},
	pmcid = {PMC1088753},
	keywords = {bottleneck, genetic diversity},
	pages = {1387--1394},
	file = { CFC23F71-84C4-45F1-83B0-D250FD34EE6F.pdf:/Users/martin/Zotero/storage/BV3KBEX8/CFC23F71-84C4-45F1-83B0-D250FD34EE6F.pdf:application/pdf;CFC23F71-84C4-45F1-83B0-D250FD34EE6F.pdf:/Users/martin/Zotero/storage/6WAZWQ7Y/CFC23F71-84C4-45F1-83B0-D250FD34EE6F.pdf:application/pdf}
