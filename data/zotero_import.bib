@article{van_heerwaarden_population_2008,
	title = {Population {Bottlenecks} {Increase} {Additive} {Genetic} {Variance} {But} {Do} {Not} {Break} a {Selection} {Limit} in {Rain} {Forest} {Drosophila}},
	volume = {179},
	url = {http://www.genetics.org/cgi/doi/10.1534/genetics.107.082768},
	doi = {10.1534/genetics.107.082768},
	abstract = {{\textless}p{\textgreater}According to neutral quantitative genetic theory, population bottlenecks are expected to decrease standing levels of additive genetic variance of quantitative traits. However, some empirical and theoretical results suggest that, if nonadditive genetic effects influence the trait, bottlenecks may actually increase additive genetic variance. This has been an important issue in conservation genetics where it has been suggested that small population size might actually experience an increase rather than a decrease in the rate of adaptation. Here we test if bottlenecks can break a selection limit for desiccation resistance in the rain forest-restricted fly \textit{Drosophila bunnanda}. After one generation of single-pair mating, additive genetic variance for desiccation resistance increased to a significant level, on average higher than for the control lines. Line crosses revealed that both dominance and epistatic effects were responsible for the divergence in desiccation resistance between the original control and a bottlenecked line exhibiting increased additive genetic variance for desiccation resistance. However, when bottlenecked lines were selected for increased desiccation resistance, there was only a small shift in resistance, much less than predicted by the released additive genetic variance. The small selection response in the bottlenecked lines was no greater than that observed in the control lines. Thus bottlenecks might produce a statistically detectable change in additive genetic variance but this change has no impact on the response to selection.{\textless}/p{\textgreater}},
	language = {English},
	number = {4},
	journal = {Genetics},
	author = {van Heerwaarden, Belinda and Willi, Yvonne and Kristensen, Torsten N and Hoffmann, Ary A},
	month = aug,
	year = {2008},
	pmid = {18689893},
	pmcid = {PMC2516085},
	keywords = {bottleneck, bottleneck intro, genetic diversity},
	pages = {2135--2146},
	file = { F8877656-4BF7-4CA6-8039-72788A18FD2A.pdf:/Users/martin/Zotero/storage/SBWRJEKZ/F8877656-4BF7-4CA6-8039-72788A18FD2A.pdf:application/pdf;F8877656-4BF7-4CA6-8039-72788A18FD2A.pdf:/Users/martin/Zotero/storage/P7RFGU8G/F8877656-4BF7-4CA6-8039-72788A18FD2A.pdf:application/pdf}
}

